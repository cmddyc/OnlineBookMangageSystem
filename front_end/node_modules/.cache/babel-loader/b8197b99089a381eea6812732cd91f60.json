AA,CAAA,KAAA,CAAA,SAAA,CAAA,EAAA,CAAA;AACA;;AALA,sBAMA,GAAA,CAAA,MAAA,KAAA,GANA;AAAA;AAAA;AAAA;;AAOA,gBAAA,KAAA,CAAA,iBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA;;AAPA,sBAQA,KAAA,CAAA,iBAAA,CAAA,MAAA,KAAA,CARA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAWA,gBAAA,KAAA,CAAA,MAAA,GAAA,IAAA;AACA,gBAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KAfA;AAgBA,IAAA,SAhBA,qBAgBA,GAhBA,EAgBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA,KADA;AAEA,kBAAA,OAAA,EAAA,GAAA,CAAA;AAFA,iBADA;AAAA;AAAA,uBAKA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,MAAA,CAAA,OAAA,GAAA,mBAAA,EAAA,MAAA,CALA;;AAAA;AAKA,gBAAA,GALA;;AAMA,oBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AACA,oBAAA,OAAA,EAAA,MADA;AAEA,oBAAA,QAAA,EAAA;AAFA,mBAAA;;AAIA,kBAAA,UAAA,CAAA,YAAA;AACA,oBAAA,MAAA,CAAA,MAAA;AACA,mBAFA,EAEA,GAFA,CAAA;AAGA,iBARA,MAQA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA;AACA,oBAAA,OAAA,EAAA,YADA;AAEA,oBAAA,QAAA,EAAA;AAFA,mBAAA;AAIA;;AAnBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBA;AApCA,GAZA;AAkDA,EAAA,OAlDA,qBAkDA;AACA,SAAA,oBAAA;AACA;AApDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"text-align:center;user-select:none;\"><span class=\"gray-title\">反馈管理</span></div>\r\n    <!-- 空状态显示区 -->\r\n    <el-empty description=\"暂无反馈\" v-if=\"!isFind\" :image-size=\"200\" style=\"margin-top: 5rem;user-select:none;\"></el-empty>\r\n    <!-- 列表显示区域 -->\r\n    <el-table class=\"book-table\" :data=\"borrowRequestList\" v-if=\"isFind\" v-loading=\"loading\" style=\"width: 100%;\">\r\n      <el-table-column type=\"index\" align=\"center\" label=\"#\"></el-table-column>\r\n      <el-table-column label=\"用户ID\" align=\"center\" prop=\"book_name\"></el-table-column>\r\n      <el-table-column label=\"反馈时间\" align=\"center\" prop=\"book_id\"></el-table-column>\r\n      <el-table-column label=\"反馈内容\" align=\"center\" prop=\"author\"></el-table-column>\r\n      <el-table-column label=\"操作\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-link type=\"success\" @click=\"agreeWith(scope.row)\">通过</el-link>\r\n            <h7>&nbsp</h7>\r\n            <el-link type=\"danger\" @click=\"disagreeWith(scope.row)\">拒绝</el-link>\r\n          </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import {\r\n    mapState,\r\n    mapMutations,\r\n    mapGetters,\r\n    mapActions\r\n  } from 'vuex'\r\n  export default {\r\n    inject: ['reload'],\r\n    data() {\r\n      return {\r\n        isFind: false,\r\n        loading: true,\r\n        borrowRequestList: [],\r\n      }\r\n    },\r\n    computed: {\r\n      ...mapState('userAbout', ['id'])\r\n    },\r\n    methods: {\r\n      async getBorrowRequestList() {\r\n        const res = await this.$http.get(this.baseUrl+`/adminGetBorrowList`)\r\n        let tmp = res.data.bookList\r\n        for (let i = 0; i < tmp.length; i++) {\r\n          tmp[i].state = tmp[i].state.substring(11)\r\n        }\r\n        if (res.status === 200) {\r\n          this.borrowRequestList = res.data.bookList\r\n          if(this.borrowRequestList.length === 0) {\r\n            return\r\n          }\r\n          this.isFind = true\r\n          this.loading = false\r\n        }\r\n      },\r\n      async agreeWith(row) {\r\n        let submit = {\r\n          id: row.state,\r\n          book_id: row.book_id\r\n        }\r\n        const res = await this.$http.post(this.baseUrl+'/adminAgreeBorrow', submit)\r\n        if (res.status === 200) {\r\n          this.$message.info({\r\n            message: '操作成功',\r\n            duration: 500\r\n          })\r\n          setTimeout(() => {\r\n            this.reload()\r\n          }, 500)\r\n        } else {\r\n          this.$message.error({\r\n            message: '操作失败,请稍后再试',\r\n            duration: 1000\r\n          })\r\n        }\r\n      }\r\n    },\r\n    created() {\r\n      this.getBorrowRequestList()\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n  .gray-title {\r\n    font-size: 2.2rem;\r\n    color: gray;\r\n    letter-spacing: 1rem;\r\n    text-shadow: 5px 5px 5px gray;\r\n  }\r\n\r\n  .book-table {\r\n    margin-top: 2rem;\r\n    border-radius: 0.5rem;\r\n  }\r\n\r\n  .admin-ctrl {\r\n    display: flex;\r\n    justify-content: space-around;\r\n  }\r\n</style>"],"sourceRoot":"src/views/admin"}]}